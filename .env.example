# Google Cloud Project Configuration
SWAPWITHUS_PROJECT_ID=your-project-id-here
SWAPWITHUS_SQL_REGION=europe-north1
SWAPWITHUS_SQL_INSTANCE=your-instance-name

# Database Configuration
SWAPWITHUS_DATABASE_NAME=your-database-name
SWAPWITHUS_DB_USER=postgres
SWAPWITHUS_DB_PASSWORD=your-secure-password-here
SWAPWITHUS_DB_HOST=your-db-host-ip
SWAPWITHUS_DATABASE_URL=postgresql+asyncpg://user:password@host:5432/database

# Google Maps API
SWAPWITHUS_MAPS_API_KEY=your-google-maps-api-key-here

# Google Cloud Storage
GOOGLE_CLOUD_STORAGE_BUCKET=your-bucket-name

# Google Cloud CDN Signing
GOOGLE_CLOUD_CDN_SIGNING_KEY=your-base64-encoded-key-here
GOOGLE_CLOUD_CDN_KEY_NAME=your-key-name

# Redis Configuration
REDIS_URL=redis://default:password@your-redis-host:port
